{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "pre-commit": "lint-staged",
    "dev": "nest start --watch --preserveWatchOutput | npm run pino-pretty ",
    "dev:debug": "nest start --debug --watch --preserveWatchOutput | npm run pino-pretty",
    "pino-pretty": "pino-pretty --hideObject -i pid,hostname -t SYS:standard -o '\u001b[33m[{req.id}]\u001b[0m \u001b[32m[{context}]\u001b[0m {msg}'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node build/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:create": "prisma migrate dev --create-only --name",
    "prisma:reset": "prisma migrate reset",
    "graphql:generate": "ts-node src/generate-typings.ts",
    "lint": "eslint --ext ts,tsx,js,jsx .",
    "lint:fix": "eslint --fix --ext ts,tsx,js,jsx .",
    "clean": "rimraf node_modules && rimraf build"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.9.0",
    "apollo-server-express": "^3.11.1",
    "apollo-server-plugin-base": "^3.7.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "convict": "^6.2.4",
    "deep-equal": "^2.2.0",
    "dotenv": "^16.0.3",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pino-http": "^8.3.1",
    "redis": "^4.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.19",
    "@types/convict": "^6.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.11",
    "jest": "28.1.2",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.3.2",
    "prisma": "^4.9.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
